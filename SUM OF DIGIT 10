import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
         int a = sc.nextInt();
          if (a < 100) { 
            System.out.println("Invalid Input");
        } else {
            int sum = 0;
            while (a > 0) {  
                int digit = a % 10; 
                sum = sum + digit; 
                a = a / 10; 
            }
            System.out.println("Sum of digit is " + sum);
        }
    }
}
